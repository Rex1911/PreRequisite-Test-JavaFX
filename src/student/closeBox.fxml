<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.closeBoxController">
   <children>
      <Label layoutX="44.0" layoutY="57.0" prefHeight="43.0" prefWidth="512.0" text="Are you sure you want to submit and finish the test?">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="cancelBtn" layoutX="362.0" layoutY="136.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="submitBtn" layoutX="486.0" layoutY="136.0" mnemonicParsing="false" onAction="#submit" text="Submit">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
